{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "POSTGRES_URL": "@postgres_url",
      "POSTGRES_PRISMA_URL": "@postgres_prisma_url",
      "POSTGRES_URL_NON_POOLING": "@postgres_url_non_pooling"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
