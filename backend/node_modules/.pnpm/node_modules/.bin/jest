#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_a844b841e4dbd299fb7c3bb8d0e6889f/node_modules/jest-cli/bin/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_a844b841e4dbd299fb7c3bb8d0e6889f/node_modules/jest-cli/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_a844b841e4dbd299fb7c3bb8d0e6889f/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_a844b841e4dbd299fb7c3bb8d0e6889f/node_modules/jest-cli/bin/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_a844b841e4dbd299fb7c3bb8d0e6889f/node_modules/jest-cli/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/jest-cli@30.2.0_@types+node_a844b841e4dbd299fb7c3bb8d0e6889f/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
