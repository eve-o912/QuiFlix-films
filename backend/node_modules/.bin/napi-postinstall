#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/napi-postinstall/lib/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/napi-postinstall/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/node_modules:/proc/cygdrive/c/Users/Mauricen/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/napi-postinstall/lib/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/napi-postinstall/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/node_modules:/proc/cygdrive/c/Users/Mauricen/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Mauricen/Desktop/quiflix-scroll/QuiFlix-films/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../napi-postinstall/lib/cli.js" "$@"
else
  exec node  "$basedir/../napi-postinstall/lib/cli.js" "$@"
fi
